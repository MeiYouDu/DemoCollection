{
	"compilerOptions": {
		"target": "esnext",
		"module": "ESNext",
		"moduleResolution": "bundler",
		"allowJs": true,
		"allowSyntheticDefaultImports": true,
		"checkJs": true,
		"jsx": "preserve",
		"types": ["fabric"],
		"strict": true,
		"noEmit": true,
		"paths": {
			"/node_modules/fabric/dist/fabric.min.mjs": [
				"./node_modules/fabric"
			]
		}
	},
	"include": ["./**/*"]
}
